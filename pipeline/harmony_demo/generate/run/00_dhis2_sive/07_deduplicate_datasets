#!/bin/bash -eu
set -o pipefail
source "${PIPELINE_UTILS_DIR}/bash/common.sh"

# This script will not only deduplicate the datasets, but also separate resample data into a
# separate file.


"${PIPELINE_SRC_ROOT}/data/pipeline/dhis2/scripts/deduplicate_dataset.py" \
  --new_file_path="${PIPELINE_TMP_DIR}" \
  --input_datasets_filepath="${PIPELINE_FEED_DIR}/dhis2_non_resampled_fields.data_sets.json" \
  --input_resample_datasets_filepath="${PIPELINE_FEED_DIR}/dhis2_resampled_fields.data_sets.json"
